    <item android:state_pressed="true"
        >
        <bitmap
            android:src="@drawable/ic_result"
            android:tint="@color/green"/>
    </item>
    <item android:drawable="@drawable/ic_result" android:state_focused="true" /> <!-- focused -->
    <item android:drawable="@drawable/ic_result" /> <!-- default -->
</selector>